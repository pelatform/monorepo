{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        ".vercel",
        ".next/**",
        "!.next/cache/**",
        "build/**",
        "dist/**",
        "out/**",
        ".source/**"
      ]
    },
    "lint": {},
    "types:check": {
      "dependsOn": [],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "persistent": false
    },
    "clean:all": {
      "cache": false,
      "persistent": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    }
  }
}
